{
  "name": "milim-server",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types/index.ts"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/main.ts",
    "start": "cross-env NODE_ENV=production node ./dist/main.js",
    "build": "tsc",
    "full-start": "npm run build && npm run start",
    "create-migration": "npx prisma migrate dev --name",
    "migrate": "npx prisma migrate deploy",
    "update-prisma-client": "npx prisma generate",
    "pm2-start": "pm2 start npm --name milim-server -- start",
    "prod": "npx prisma generate && npm run build && npm run pm2-start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
